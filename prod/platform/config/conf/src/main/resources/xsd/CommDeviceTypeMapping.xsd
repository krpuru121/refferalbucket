<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    vc:minVersion="1.1" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning">
    <xs:include schemaLocation="Common.xsd"/>
    <xs:complexType name="commDeviceType">
        <xs:sequence>
            <xs:element name="commDeviceTypeName" type="xs:string"/>
            <xs:element name="commDeviceDriverClass" type="xs:string"/>
            <xs:element ref="nameValue" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="extension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="commDeviceTypeList">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="commDeviceType" type="commDeviceType" maxOccurs="unbounded"/>
                <xs:element ref="extension" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
